{
  "name": "webpack_scss_babel_boilerplate",
  "version": "1.1.0",
  "description": "A Webpack boilerplate for html, scss and babel.js",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --config webpack/webpack.dev.js",
    "build:prod": "webpack --config webpack/webpack.prod.js",
    "serve": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "clear": "rm -rf dist/",
    "clear:dev": "rm -rf dist/development",
    "clear:prod": "rm -rf dist/production",
    "clear:all": "rm -rf node_modules/ && npm run clear:dist",
    "set:bs:css": "npm install --save-dev bootstrap",
    "set:bs:all": "npm install --save-dev bootstrap jquery popper.js",
    "bs:clear": "npm uninstall --save-dev bootstrap jquery popper.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wangchimei/boilerplate-webpack-scss-babel.git"
  },
  "keywords": [
    "boilerplate",
    "scss",
    "webpack",
    "babel",
    "html"
  ],
  "author": "Wangchimei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wangchimei/boilerplate-webpack-scss-babel/issues"
  },
  "homepage": "https://github.com/Wangchimei/boilerplate-webpack-scss-babel#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.1.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
